<form class="change-info-form">
  <div class="form-item">
    <input
      type="text"
      autocomplete="off"
      id="check-in"
      class="form-control form-input isEmpty"
      placeholder="Check-in"
      readonly
    />
  </div>
  <div class="form-item">
    <input
      type="text"
      autocomplete="off"
      id="check-out"
      class="form-control form-input isEmpty"
      placeholder="Check-out"
      readonly
    />
  </div>
  <div class="form-item">
    <input
      class="form-control form-input form-select"
      id="guestNum"
      type="number"
      min="1"
      max="50"
      size="4"
      maxlength="4"
      placeholder="Ospiti"
    />
  </div>
</form>
